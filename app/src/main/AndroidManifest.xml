<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scheduling_app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ConfiguracaoActivity"></activity>
        <activity android:name=".Atividades.GUI.Calendario2Activity" />
        <activity android:name=".Atividades.GUI.AtividadeDetalhesActivity" />
        <activity android:name=".Atividades.GUI.CalendarioActivity" />
        <activity android:name=".Atividades.GUI.AtividadesActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Atividades.GUI.cadastroAtividadeActivity" />
        <activity android:name=".User.GUI.CriarContaActivity" />
        <activity android:name=".User.GUI.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>